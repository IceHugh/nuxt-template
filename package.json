{
  "name": "nuxt-template",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "build:cf": "NITRO_PRESET=cloudflare-pages nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check --write app server modules i18n shared",
    "biome:check": "biome check .",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:local:migrate": "NODE_ENV=development bun run db:migrate",
    "db:local:generate": "NODE_ENV=development bun run db:generate",
    "db:local:studio": "NODE_ENV=development bun run db:studio",
    "db:reset": "rm -f ./data/app.db && tsx server/scripts/init-db.ts && bun run db:seed",
    "db:init": "tsx server/scripts/init-db.ts",
    "db:seed": "tsx server/scripts/seed.ts"
  },
  "dependencies": {
    "@nuxt/fonts": "0.11.4",
    "@nuxt/image": "1.11.0",
    "@nuxt/scripts": "0.13.0",
    "@nuxt/test-utils": "3.20.1",
    "@nuxtjs/color-mode": "^3.5.2",
    "@trpc/client": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@unhead/vue": "^2.0.3",
    "@unovis/ts": "^1.6.1",
    "@unovis/vue": "^1.6.1",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^14.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.31.6",
    "drizzle-orm": "^0.44.7",
    "embla-carousel-vue": "^8.6.0",
    "lucide-vue-next": "^0.548.0",
    "nuxt": "^4.2.0",
    "pinia": "^3.0.3",
    "radash": "^12.1.1",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "vue-sonner": "^2.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.1",
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/lucide": "^1.2.71",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/solar": "^1.2.5",
    "@iconify-json/tabler": "^1.2.23",
    "@nuxt/icon": "^2.1.0",
    "@nuxtjs/i18n": "^10.1.2",
    "@pinia/nuxt": "^0.11.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.9.1",
    "better-sqlite3": "^12.4.1",
    "husky": "^9.1.7",
    "shadcn-nuxt": "^2.3.2",
    "tsx": "^4.20.6",
    "zod": "^4.1.12"
  }
}
