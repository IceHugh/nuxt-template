# 环境变量示例文件
# 复制为 .env 并填入实际值

# 应用配置
NUXT_PUBLIC_APP_NAME=Nuxt 4 Template
NUXT_PUBLIC_APP_VERSION=1.0.0

# API 配置
NUXT_PUBLIC_API_URL=http://localhost:3000

# 第三方服务
NUXT_PUBLIC_ANALYTICS_ID=

# Cloudflare D1 数据库配置
CLOUDFLARE_ACCOUNT_ID=your_account_id
CLOUDFLARE_DATABASE_ID=1f3e3c81-d302-4ee7-b3b0-5304e4d99ced
CLOUDFLARE_D1_TOKEN=your_d1_api_token

# 数据库配置（D1）
DATABASE_URL=

# Cloudflare Pages
NUXT_CF_PAGES=1

# 开发模式
NODE_ENV=development